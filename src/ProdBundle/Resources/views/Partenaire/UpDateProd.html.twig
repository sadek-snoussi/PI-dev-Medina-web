{% extends ":client:layout.html.twig" %}
{% block flexslider %}
{% endblock %}


{% block content %}


    <div class="container">

        <div class="col-md-4 col-md-offset-4">

            {{ form_start(form) }}

            <div class="form-group">
                {{ form_widget(form.nomproduit, {'attr': {'class': 'form-control','required' : 'true'}}) }}
            </div>

            <div class="form-group">
                {{ form_widget(form.referenceProd, {'attr': {'class': 'form-control','required' : 'true'}}) }}
            </div>

            <div class="form-group">
                {{ form_widget(form.matiereproduit, {'attr': {'class': 'form-control','required' : 'true'}}) }}
            </div>

            <div class="form-group">
                {{ form_widget(form.dateexpirationproduit, {'attr': {'class': 'datepicker','required' : 'true'}} ) }}
            </div>


            <div class="form-group">
                {{ form_widget(form.Ajouter, {'attr': {'class': 'btn btn-warning'}}) }}
            </div>

            {{ form_end(form) }}
        </div>

    </div>
































{% endblock content %}